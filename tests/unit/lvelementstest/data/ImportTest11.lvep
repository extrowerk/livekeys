// This file holds an import as plugin from a main file

////// plugin1/live.plugin.json

{
    "name" : "plugin1",
    "package": "..",
    "modules": ["A", "B"]
}

////// plugin1/A.lv

component default < Element{
    fn toString(){
        return "class[A]"
    }
}


////// plugin1/B.lv

component default < Element{
    fn toString(){
        return "class[B]"
    }
}

////// live.package.json

{
    "name" : "main",
    "version" : "1.0.0"
}

////// live.plugin.json

{
    "name" : "main",
    "package": ".",
    "modules": ["main"]
}

////// main.lv

import .plugin1 as plug

instance default Element{
    string a : {
        return (new plug.A()).toString()
    }
    string b : {
        return (new plug.B()).toString()
    }
}
