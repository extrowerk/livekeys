// This file holds a dependency cycle between plugins within a package
// plugin1 <- plugin2 <- plugin3 <- plugin1

////// live.package.json

{
    "name" : "main",
    "version" : "1.0.0"
}

////// live.module.json

{
    "name" : "main",
    "package" : "."
}

////// main.lv

import .plugina

instance default A{
}


////// plugina/live.module.json

{
    "name" : "plugina",
    "package": "..",
    "modules": ["A"]
}

////// plugina/A.lv

import .pluginb

component default < B {}

////// pluginb/live.module.json

{
    "name" : "pluginb",
    "package": "..",
    "modules": ["B"]
}

////// pluginb/B.lv

import .pluginc

component default < C {}

////// pluginc/live.module.json

{
    "name" : "pluginc",
    "package": "..",
    "modules": ["C"]
}

////// pluginc/C.lv

import .plugina

component C < A {}

