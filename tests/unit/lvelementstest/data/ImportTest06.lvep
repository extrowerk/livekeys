// This file holds a simple import of a plugin from a main file.

////// plugin1/live.plugin.json

{
    "name" : "plugin1",
    "package": "..",
    "modules": ["A", "B", "C"]
}

////// plugin1/A.lv

component default < Element{
    fn toString(){
        return "class[A]"
    }
}


////// plugin1/B.lv

component default < Element{
    fn toString(){
        return "class[B]"
    }
}

////// plugin1/C.lv

component default < Element{
    fn toString(){
        return "class[C]"
    }
}

////// live.package.json

{
    "name" : "test-package",
    "version" : "1.0.0"
}

////// live.plugin.json

{
    "name" : "main",
    "package": ".",
    "modules": ["main"]
}


////// main.lv

import .plugin1
import .plugin1 as p1
import .plugin1 as p2

instance default Element{
    string a : {
        return (new A()).toString()
    }
    string b : {
        return (new p1.B()).toString()
    }
    string c : {
        return (new p2.C()).toString()
    }
}

